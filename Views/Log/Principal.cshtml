@model IEnumerable<Proyecto.Models.Usuarios>

@{
    ViewBag.Title = "Principal";
}

<h2>Principal</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nomUsuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.apellido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.correo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.contrasena)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nomUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.apellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.correo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.contrasena)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.idUsuario }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.idUsuario })
            </td>
        </tr>
    }

</table>